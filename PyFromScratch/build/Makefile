OBJS = ast.cmo astpp.cmo parser.cmo scanner.cmo compile.cmo  main.cmo

py++ : $(OBJS)
	ocamlc -o py++ $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

astpp.ml : ast.ml
	ast-pp "Ast" < ast.ml > astpp.ml 

% : ../%
	cp $< $*

.PHONY : clean
clean :
	rm -f py++ *.ml *.cmo *.cmi *.mli *.mly *.mll
	cp ../*.* .

# Generated by ocamldep *.ml *.mli
ast.cmo:
ast.cmx:
compile.cmo: ast.cmo astpp.cmo
compile.cmx: ast.cmx astpp.cmo
main.cmo: scanner.cmo parser.cmi compile.cmo ast.cmo
main.cmx: scanner.cmx parser.cmx compile.cmx ast.cmx
parser.cmo: ast.cmo parser.cmi
parser.cmx: ast.cmx parser.cmi
scanner.cmo: parser.cmi
scanner.cmx: parser.cmx
parser.cmi: ast.cmo
